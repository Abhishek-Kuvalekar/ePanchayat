{
  "name": "ePanchayat",
  "version": "1.0.0",
  "husky": {
    "hooks": {
      "pre-commit": "npm run verify"
    }
  },
  "scripts": {
    "ng": "ng",
    "lint": "eslint -c .eslintrc.js --ext .ts ./src",
    "start": "node --max_old_space_size=8192 ./node_modules/@angular/cli/bin/ng serve",
    "build": "node --max_old_space_size=8192 ./node_modules/@angular/cli/bin/ng build --configuration development",
    "build-prod": "node --max_old_space_size=8192 ./node_modules/@angular/cli/bin/ng build --base-href /ePanchayat/ --configuration production",
    "build-vendors": "ngcc --properties es2015 browser module main --create-ivy-entry-points",
    "postinstall": "npm run build-vendors",
    "analyze": "webpack-bundle-analyzer dist/ePanchayat/stats.json",
    "prettier": "prettier --write \"./**/*.{html,js,jsx,mjs,cjs,ts,tsx,json}\""
  },
  "private": true,
  "browser": {
    "fs": false,
    "path": false,
    "os": false,
    "stream": false,
    "zlib": false,
    "http": false,
    "https": false
  },
  "browserList": [
    "edge >= 16",
    "chrome >= 62"
  ],
